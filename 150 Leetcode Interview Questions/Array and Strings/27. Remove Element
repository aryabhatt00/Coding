#Java
class Solution {
    public int removeElement(int[] nums, int val) {

        int k=0;
        for(int i=0;i<nums.length;i++)
        {
            if (nums[i]!=val)
            {
             nums[k]=nums[i];
             k++; 
            }
        }
        return k;
    }
}

#Python
class Solution:
    def removeElement(self, nums: List[int], val: int) -> int:
        n=nums.count(val)
        z=0
        for i in range(n):
            nums.remove(val)
        for i in range(len(nums)):
            if nums[i]=="":
                z+=1
        return(len(nums)-z)
